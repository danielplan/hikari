{
  "name": "hikari",
  "version": "0.0.0",
  "homepage": "http://danielplan.github.io/hikari",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "compile": "emcc src/editor/render/wasm.c -o src/editor/render/wasm.js -s EXTRA_EXPORTED_RUNTIME_METHODS=['ccall','cwrap'] -s EXPORT_ES6=1 -s MODULARIZE=1"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "sass": "^1.43.4",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  }
}